{
  "name": "ts-synth",
  "version": "1.0.0",
  "description": "A simple web audio synthesizer written in vanilla TypeScript",
  "source": "index.html",
  "scripts": {
    "start:parcel": "npx parcel --port 4321",
    "start": "npm run start:parcel",
    "build": "npm run cleanup && npx parcel build --public-url ./",
    "push-live": "npm run build && push-dir --dir=dist --branch=gh-pages --cleanup --verbose",
    "cleanup": "rm -rf dist .parcel-cache"
  },
  "keywords": [
    "typescript",
    "web-audio",
    "synthesizer",
    "web-development",
    "javascript",
    "html",
    "css"
  ],
  "author": "Jason Sherrick",
  "license": "MIT",
  "devDependencies": {
  },
  "dependencies": {
  }
}
